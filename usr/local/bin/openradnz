#!/bin/bash
# OpenRA Dark Reign latest commit available
cd /home/fusion809/GitHub/others/DarkReign
git pull origin master -q
orlv=$(git rev-list --branches master --count)
comm=$(git log -1 --format=%h)
# OpenRA latest packaged version
orpv=$(cat $HOME/OBS/home:fusion809/openra-dr/openra-dr.spec | grep "Version:" | sed 's/Version:\s*//g')

if ! [[ $orpv == $orlv ]]; then
    zenity --error --text="A new OpenRA DarkReign mod commit (${pkgver}) is available, to upgrade to it run drup\!" --title="Warning\!"
fi
