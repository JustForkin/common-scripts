#!/bin/bash
# OpenBabel latest version available
cd /home/fusion809/GitHub/others/openbabel
git pull origin master -q
orlv=$(git rev-list --branches master --count)
# OpenBabel latest packaged version
orpv=$(cat $HOME/OBS/home:fusion809:openbabel3/openbabel3/openbabel3.spec | grep "Version:" | sed 's/Version:\s*//g')

if [[ $orpv == $orlv ]]; then
    printf "Open Babelï…¤ \n"
else
    printf "Open Babel${orlv}\n"
fi
