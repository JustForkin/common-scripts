#!/bin/bash
pkgver=$(curl -sL https://github.com/openSUSE/osc/releases | grep "\.tar\.gz" | head -n 1 | cut -d '"' -f 2 | cut -d '/' -f 5 | sed 's/\.tar\.gz//g')
cd $HOME/OBS/home:fusion809/osc
pkgpver=$(sed -n 's/pkgver=//p' PKGBUILD)

if ! ( [ $pkgpver == $pkgver ] ); then
    printf "osc$pkgver\n"
else
    printf "oscï…¤\n"
fi
